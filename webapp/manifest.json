{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.krypt.DemoApp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.krypt.DemoApp.view.Worklist",
			"type": "XML",
			"async": true,
			"id": "Worklist"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.krypt.DemoApp.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"config": {
			"fullWidth": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.krypt.DemoApp.view",
				"controlId": "fcl",
				"transition": "slide"
			},
			"routes": [{
				"pattern": ":layout:",
				"name": "Worklist",
				"target": [
					"Worklist",
					"Object"
				]
			}, {
				"pattern": "Object/{product}/{layout}",
				"name": "Object",
				"target": [
					"Worklist",
					"Object"
				]
			}, {
				"pattern": "Object2/{product}/{supplier}/{layout}",
				"name": "Object2",
				"target": [
					"Worklist",
					"Object",
					"Object2"
				]
			}],
			"targets": {
				"Worklist": {
					"viewType": "XML",
					"viewName": "Worklist",
					"controlAggregation": "beginColumnPages"
				},
				"App": {
					"viewType": "XML",
					"viewName": "App"
				},
				"Object": {
					"viewType": "XML",
					"viewName": "Object",
					"controlAggregation": "midColumnPages"
				},
				"Object2": {
					"viewType": "XML",
					"viewName": "Object2",
					"controlAggregation": "endColumnPages"
				}
			}
		}
	}
}